<testStep>
	<description>Test harness requests the receiver to explicitly give it the TEST domain identifiers by sending:</description>
	<message>MSH|^~\&amp;|TEST_HARNESS|TEST|CR1|MOH_CAAT|20090223144546|TEST_HARNESS+TEST_HARNESS|QBP^Q23^QBP_Q21|TEST-CR-10-20|P|2.5
QPD|IHE PIX Query|Q1020|RJ-444^^^TEST^PI|^^^TEST
RCP|I</message>
	<assertions>
		<assert terserString="MSA-1" value="AA" alternate="acknowledgement" />
		<assert terserString="MSA-1" value="CA" alternate="acknowledgement" />
		<assert terserString="QUERY_RESPONSE/.PID-3(0)-1" value="RJ-444" alternate="id" />
		<assert terserString="QUERY_RESPONSE/.PID-3(1)-1" value="RJ-444" alternate="id" />
		<assert terserString="QUERY_RESPONSE/.PID-3(0)-4-1" value="TEST" alternate="namespaceId" />
		<assert terserString="QUERY_RESPONSE/.PID-3(1)-4-1" value="TEST" alternate="namespaceId" />
		<assert terserString="QUERY_RESPONSE/.PID-3(0)-4-2" value="2.16.840.1.113883.3.72.5.9.1" alternate="universalId" />
		<assert terserString="QUERY_RESPONSE/.PID-3(1)-4-2" value="2.16.840.1.113883.3.72.5.9.1" alternate="universalId" />
		<assert terserString="QUERY_RESPONSE(1)/.PID" missing="true" />
		<assert terserString="QAK-2" value="OK" />
		
		
		
        	
	</assertions>
</testStep>