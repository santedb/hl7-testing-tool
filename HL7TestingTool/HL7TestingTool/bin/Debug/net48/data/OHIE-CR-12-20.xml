<testStep>
	<description>Test harness sends a PDQ message containing a name which is known to the receiver:</description>
	<message>MSH|^~\&amp;|TEST_HARNESS|TEST|CR1|MOH_CAAT|20090226131520-0600|TEST_HARNESS+TEST_HARNESS|QBP^Q22^QBP_Q21|TEST-CR-12-20|P|2.5
QPD|Q22^Find Candidates^HL7|Q1220|@PID.5.1^JONES~@PID.5.2^JENNIFER
RCP|I|10^RD</message>
	<assertions>
		<assert terserString="MSA-1" value="AA" alternate="true" />
		<assert terserString="MSA-1" value="CA" alternate="true" />
		<assert terserString="QUERY_RESPONSE/.PID-3(0)-1" value="RJ-439" alternate="true" />
		<assert terserString="QUERY_RESPONSE/.PID-3(1)-1" value="RJ-439" alternate="true" />
		<assert terserString="QUERY_RESPONSE/.PID-3(0)-4" value="TEST" alternate="true" />
		<assert terserString="QUERY_RESPONSE/.PID-3(1)-4" value="TEST" alternate="true" />
		<assert terserString="QUERY_RESPONSE/.PID-5-1" value="JONES" />
		<assert terserString="QUERY_RESPONSE/.PID-5-2" value="JENNIFER" />
		<assert terserString="QUERY_RESPONSE/.PID-7" value="19840125" />
		<assert terserString="QAK-2" value="OK" />
		
        	
	</assertions>
</testStep>