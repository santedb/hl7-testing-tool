{
  "Serilog": {
    "Using": ["Serilog.Sinks.Console", "Serilog.Sinks.File"],
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "Enrich": ["FromLogContext"],
    "WriteTo": [
      {
        "Name": "Console",
        "Args": {
          "outputTemplate": "[{Timestamp:HH:mm:ss} {Level}] {SourceContext} {Message:lj}{NewLine}{Exception}"
        }
      },
      {
        "Name": "File",
        "Args": {
          "path": "hl7_testing_tool_.log",
          "rollingInterval": "Day"
        }
      }
    ]
  },
  "Environment": "Development",
  "Endpoint": "mllp://127.0.0.1:2100",
  "TestDirectory": "data",
  "TestOptions": {
    "Execution": ["OHIE-CR-02-20"]
  }
}